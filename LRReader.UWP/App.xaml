<Application
	x:Class="LRReader.UWP.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:controls="using:Microsoft.UI.Xaml.Controls"
	xmlns:local="using:LRReader.UWP"
	xmlns:vm="using:LRReader.UWP.ViewModels">
	<Application.Resources>
		<ResourceDictionary>
			<vm:ViewModelLocator x:Key="Locator" />
			<ResourceDictionary.MergedDictionaries>
				<controls:XamlControlsResources ControlsResourcesVersion="Version2">
					<controls:XamlControlsResources.MergedDictionaries>
						<ResourceDictionary Source="/Resources/ThemeDictionary.xaml" />
						<ResourceDictionary Source="/Resources/NotificationStyle.xaml" />
					</controls:XamlControlsResources.MergedDictionaries>
				</controls:XamlControlsResources>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>

</Application>
