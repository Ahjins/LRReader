<controls:TabViewItem
	x:Class="LRReader.UWP.Core.Views.Tabs.WebTab"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:content="using:LRReader.UWP.Core.Views.Tabs.Content"
	xmlns:controls="using:Microsoft.UI.Xaml.Controls"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="using:LRReader.UWP.Core.Views.Tabs"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	d:DesignHeight="300"
	d:DesignWidth="400"
	Header="{x:Bind TabContent.ViewModel.Title, Mode=OneWay}"
	mc:Ignorable="d" Loaded="TabViewItem_Loaded">

	<content:WebTabContent x:Name="TabContent" />
</controls:TabViewItem>
